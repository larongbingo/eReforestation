{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "copyfiles \"src/**/*.html\" dist/server",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/server/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.1.2",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/terminus": "^6.5.0",
    "archiver": "^3.1.1",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.19.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.0.0",
    "nest-router": "^1.0.9",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.3.3",
    "sequelize": "4",
    "sequelize-typescript": "0.6.10",
    "swagger-ui-express": "^4.1.1",
    "unzip": "^0.1.11",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/archiver": "^3.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.4",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^12.7.8",
    "@types/nodemailer": "^6.2.0",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-jwt": "^3.0.1",
    "@types/randomstring": "^1.1.6",
    "@types/rimraf": "^2.0.2",
    "@types/sequelize": "4.28.6",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.7",
    "@types/unzip": "^0.1.1",
    "concurrently": "^5.0.0",
    "copyfiles": "^2.1.1",
    "jest": "^24.8.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.0",
    "tslint-config-security": "^1.16.0",
    "typescript": "3.6.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/modules/auth/tests/mocks",
      "<rootDir>/modules/auth/tests/constants"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
